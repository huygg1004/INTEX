@{ ViewBag.Title = "View Burial Site For Public Users"; }

@*@model IEnumerable<Intex_app.Models.LocationMeasurement>*@

@model IEnumerable<Intex_app.Models.Photo>

<header class="masthead" style="background-image: url('https://cdn-ol.niceshops.com/upload/image/product/facebook/default/europet-skull-head-1-st-138290-en.jpg') ">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="site-heading">
                    <h1>View Photos</h1>
                    <span class="subheading">Your Uploaded Photos will be displayed here</span>
                </div>
            </div>
        </div>
    </div>
</header>



<img src="@ViewBag.Url" width="300" height="300" />
@foreach (Intex_app.Models.Photo x in Model)
{
    <h3>@x.PhotoID.</h3>
    <h3>@x.BurialSiteID</h3>
    <img src="@x.photoUrl" width="300" height="300" />
}